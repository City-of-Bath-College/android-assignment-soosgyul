<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="questionapp.gyula.gs.com.questionapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Placement Test App"
        android:theme="@style/AppTheme" >
        <activity
            android:name="questionapp.gyula.gs.com.questionapp.MainActivity"
            android:label="Questions"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="questionapp.gyula.gs.com.questionapp.ProfileCard"
            android:label="About the author"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="questionapp.gyula.gs.com.questionapp.IntroductionActivity"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="questionapp.gyula.gs.com.questionapp.HighScoreActivity"
            android:label="High scores"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>